{
  "name": "sief-ds1",
  "version": "0.1.3",
  "description": "A UI component library built with Subframe UI, Tailwind CSS 4, and Next.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./styles": "./dist/styles/globals.css",
    "./styles.css": "./dist/styles/globals.css",
    "./styles/theme": "./dist/styles/theme.css",
    "./styles/theme.css": "./dist/styles/theme.css"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sync:subframe": "subframe sync --all"
  },
  "keywords": [
    "react",
    "nextjs",
    "subframe",
    "ui",
    "components",
    "tailwindcss"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seifui/sampath-ui-library.git"
  },
  "homepage": "https://github.com/seifui/sampath-ui-library#readme",
  "bugs": {
    "url": "https://github.com/seifui/sampath-ui-library/issues"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0"
  },
  "dependencies": {
    "@subframe/core": "^1.148.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/builder-webpack5": "^8.6.14",
    "@storybook/preset-react-webpack": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@subframe/cli": "^1.194.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.22",
    "babel-loader": "^10.0.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.1.17",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "sideEffects": [
    "**/*.css"
  ]
}
